                    <name>ra</name>
                    <description>Right ascension</description>
                    <unit>deg</unit>
                    <ucd>pos.eq.ra;meta.main</ucd>
                    <type>float</type>
                </column>
                <column>
                    <name>dec</name>
                    <description>Declination</description>
                    <unit>deg</unit>
                    <ucd>pos.eq.dec;meta.main</ucd>
                    <type>float</type>
                </column>
            </columns>
        </expected>
    </query>
